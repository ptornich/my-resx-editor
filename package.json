      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "img/icon.icns"
      },
      "electronWinstallerConfig": {
        "name": "MyResxEditor"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "MyResxEditor",
        "name": "MyResxEditor"
      }
    }
